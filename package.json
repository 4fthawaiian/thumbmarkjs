{
  "name": "thumbmarkjs",
  "version": "0.9.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prod": "npx webpack --mode production",
    "build": "npx webpack && cp ./build/ThumbMark.js ./testpage",
    "publish": "aws s3 cp ./build/ThumbMark.js s3://ilkka-public/thumbmark/$npm_package_version/ThumbMark.js --profile ilkka",
    "latest": "npm run publish && aws s3 cp ./build/ThumbMark.js s3://ilkka-public/thumbmark/latest/ThumbMark.js  --profile ilkka",
    "testpage": "aws s3 sync ./testpage s3://thumbmarkjs.com",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E24XBAADLODIDB --paths \"/latest/ThumbMark.js\"",
    "testpage-invalidate": "aws cloudfront create-invalidation --distribution-id E1IK40S4U87D63 --paths \"/*\""
  },
  "keywords": [
    "fingerprint"
  ],
  "author": "Ilkka Peltola",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@types/ua-parser-js": "^0.7.39",
    "@types/webpack-env": "^1.18.4",
    "@webpack-cli/generators": "^3.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "path": "^0.12.7",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-md5": "^1.3.1",
    "typescript": "^5.3.3",
    "ua-parser-js": "^1.0.37",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
